{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "module": "commonjs",
    "lib": [
      "es2020"
    ],
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "declaration": true,
    "strict": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "outDir": "cdk.out"
  },
  "exclude": [
    "node_modules",
    "cdk.out"
  ]
}
